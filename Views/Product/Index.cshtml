@model IEnumerable<Machine_proj.Models.Product>

@{
    ViewBag.Title = "Product List";
}

<h2>Product List</h2>

<table class="table">
    <tr>
        <th>Product ID</th>
        <th>Product Name</th>
        <th>Product Code</th>
        <th>Product Price</th>
    </tr>

    @foreach (var product in Model) 
    {
        <tr>
            <td>@product.ProductId</td>
            <td>@product.ProductName</td>
            <td>@product.ProductCode</td>
            <td>@product.ProductPrice</td>
        </tr>
    }
</table>
